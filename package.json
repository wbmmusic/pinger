    "dependencies": {
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@mui/icons-material": "^5.6.1",
        "@mui/material": "^5.6.1",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.1.1",
        "@testing-library/user-event": "^14.1.1",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.24",
        "@types/react": "^18.0.5",
        "@types/react-dom": "^18.0.1",
        "bootstrap": "^5.1.3",
        "date-and-time": "^2.3.1",
        "electron-updater": "^5.0.1",
        "foreman": "^3.0.1",
        "nodemailer": "^6.7.3",
        "ping": "^0.4.1",
        "react": "^18.0.0",
        "react-bootstrap": "^2.2.3",
        "react-dom": "^18.0.0",
        "react-router-dom": "^6.3.0",
        "react-scripts": "5.0.1",
        "uuid": "^8.3.2",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "dev": "nf start",
        "install": "electron-rebuild",
        "rebuild": "electron-rebuild -f",
        "package": "electron-builder build -c.extraMetadata.main=build/main.js --publish never",
        "deploy": "electron-builder build -c.extraMetadata.main=build/main.js --publish always",
        "backup": "git add . && git commit -m %NPM_PACKAGE_VERSION% && git push origin master",
        "upload": "yarn build && git add . && git commit -m %NPM_PACKAGE_VERSION% && git push origin master && yarn deploy ",
        "electron": "electron .",
        "try": "yarn build && yarn package ",
        "refresh": "yarn upgrade --latest && yarn rebuild"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "electron": "^18.0.4",
        "electron-builder": "^23.0.3",
        "electron-rebuild": "^3.2.7"
    },
    "author": "Marece Williams",
    "description": "Enter a description",
    "homepage": "./",
    "main": "public/main.js",
    "build": {
        "productName": "Pinger",
        "appId": "com.wbm.pinger",
        "copyright": "WBM Tek",
        "extends": null,
        "files": [
            "build/**/*",
            "electron/*"
        ],
        "win": {
            "icon": "public/favicon.ico"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/wbmmusic/pinger.git"
    }
}
